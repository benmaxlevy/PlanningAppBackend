{
  "extends": ["plugin:prettier/recommended", "airbnb-base"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "quotes": [2, "double", "avoid-escape"],
    "indent": ["error", 4],
    "implicit-arrow-linebreak": "off",
    "no-unused-vars": "off",
    "comma-dangle": "off"
  }
}
